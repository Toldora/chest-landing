<header class='app-navbar'>
  <div class='app-navbar__container container'>
    <a class='app-navbar__logo-link' href='#' aria-label='Pagina inicial'>
      {{> svg-icon class='app-navbar__logo' width="128" height="25"
      name='logo-large' }}
    </a>

    <ul class='app-navbar__social-list'>
      {{#each list}}
        <li class='app-navbar__social-list-item'>
          <a
            class='app-navbar__social-link'
            href='{{href}}'
            target='_blank'
            rel='nofollow noopener noreferrer'
            aria-label='{{icon}}'
          >
            {{> svg-icon width="24" height="24" name=(lookup . 'icon') }}
          </a>
        </li>
      {{/each}}
    </ul>

    <button
      class='app-navbar__sign-up-btn app-button js-sign-up-btn'
      type='button'
      aria-label='Inscrever-se'
    >
      Inscrever-se
    </button>
    <button
      class='app-navbar__burger-btn js-burger-btn'
      type='button'
      aria-label='Menu'
    >
      <svg
        class='app-navbar__burger-icon'
        aria-hidden='true'
        viewBox='0 0 24 24'
        xmlns='http://www.w3.org/2000/svg'
        fill='currentColor'
      >
        <rect
          class='app-navbar__burger-rect'
          width='18'
          height='1.5'
          ry='0.75'
          x='3'
          y='6.25'
        ></rect>
        <rect
          class='app-navbar__burger-rect'
          width='18'
          height='1.5'
          ry='0.75'
          x='3'
          y='11.25'
        ></rect>
        <rect
          class='app-navbar__burger-rect'
          width='18'
          height='1.5'
          ry='0.75'
          x='3'
          y='16.25'
        ></rect>
      </svg>
    </button>
  </div>
</header>